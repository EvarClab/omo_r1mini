<launch>
  <arg name="set_port" default="/dev/ttyTHS1"/>
  <arg name="set_multi_robot_name" default=""/>
  <node pkg="omo_r1mini_bringup" type="omo_r1mini_node.py" name="$(arg set_multi_robot_name)omo_r1mini_node" output="screen">
      <param name="port" value="$(arg set_port)"/>
      <param name="baud" value="115200"/>
      <param name="modelName" value="r1mini"/>
      <param name="tf_prefix" type="string" value="$(arg set_multi_robot_name)"/>
  </node>
  <rosparam command="load" file="$(find omo_r1mini_bringup)/config/robot_config.yaml" />
</launch>
