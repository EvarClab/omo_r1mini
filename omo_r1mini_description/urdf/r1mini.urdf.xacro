<?xml version="1.0" ?>
<robot name="r1mini" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find omo_r1mini_description)/urdf/common_properties.xacro"/>
  <xacro:include filename="$(find omo_r1mini_description)/urdf/r1mini.gazebo.xacro"/>

  <link name="base_footprint"/>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link" />
    <origin xyz="0 0.0 0.0115" rpy="0 0 0.0"/>
  </joint>

  <link name="base_link">
    <visual>
      <origin xyz="0.03 -0.065 0.0" rpy="0 0 1.57"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/bases/mini_frame.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="blue"/>
    </visual>

    <collision>
      <origin xyz="0.03 -0.065 0.0" rpy="0 0 1.57"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/bases/mini_frame.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.3729096e+00"/>
      <inertia ixx="8.7002718e-03" ixy="-4.7576583e-05" ixz="1.1160499e-04"
               iyy="8.6195418e-03" iyz="-3.5422299e-06"
               izz="1.4612727e-02" />
    </inertial>
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left_link"/>
    <origin xyz="0.0 0.096 0.022" rpy="0.0 0.0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="wheel_left_link">
    <visual>
      <origin xyz="-0.0335 -0.004 -0.0335" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/wheels/mini_tire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>

    <collision>
      <origin xyz="-0.0335 -0.004 -0.0335" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/wheels/mini_tire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
    </inertial>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right_link"/>
    <origin xyz="0.0 -0.096 0.022" rpy="0.0 0.0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="wheel_right_link">
    <visual>
      <origin xyz="-0.0335 0.004 0.0335" rpy="-1.57 0 0"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/wheels/mini_tire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="-0.0335 0.004 0.0335" rpy="-1.57 0 0"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/wheels/mini_tire.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="2.8498940e-02" />
      <inertia ixx="1.1175580e-05" ixy="-4.2369783e-11" ixz="-5.9381719e-09"
               iyy="1.1192413e-05" iyz="-1.4400107e-11"
               izz="2.0712558e-05" />
    </inertial>

  </link>

  <joint name="caster_back_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster_back_link"/>
    <origin xyz="-0.12 0.0 0.0" rpy="0 0 0"/>
  </joint>

  <link name="caster_back_link">
    <visual>
      <origin xyz="-0.016 -0.007 -0.016" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/wheels/mini_caster.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="dark"/>
    </visual>
    <collision>
      <origin xyz="-0.016 -0.007 -0.016" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://omo_r1mini_description/meshes/wheels/mini_caster.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.005" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0"
               iyy="0.001" iyz="0.0"
               izz="0.001" />
    </inertial>
  </link>

</robot>